    "babel-preset-es2015": "6.24.1",
    "chai": "4.2.0",
    "eslint": "5.16.0",
    "eslint-config-dhis2": "3.0.7",
    "eslint-plugin-react": "7.12.4",
    "karma": "4.1.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage": "1.1.2",
